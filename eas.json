{
  "cli": {
    "version": ">= 16.17.4",
    "requireCommit": true,
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "ENVFILE": ".env.development"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "ENVFILE": ".env.preview"
      }
    },
    "production": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "autoIncrement": true,
      "env": {
        "ENVFILE": ".env.production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
